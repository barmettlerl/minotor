{
  "name": "minotor",
  "version": "0.0.2-beta",
  "description": "A lightweight client-side transit routing library.",
  "keywords": [
    "minotor",
    "raptor",
    "transit",
    "planner",
    "transport",
    "router",
    "gtfs",
    "trains",
    "netex"
  ],
  "homepage": "https://minotor.dev",
  "bugs": "https://github.com/aubryio/minotor/issues",
  "author": "Aubry Cholleton <aubry@cholleton.com> (https://aubry.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aubryio/minotor.git"
  },
  "type": "module",
  "main": "dist/router.cjs.js",
  "module": "dist/router.esm.js",
  "browser": "dist/router.umd.js",
  "types": "./dist/router.d.ts",
  "bin": {
    "minotor": "./dist/cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/router.esm.js",
      "require": "./dist/router.cjs.js",
      "types": "./dist/router.d.ts"
    },
    "./parser": {
      "import": "./dist/parser.esm.js",
      "require": "./dist/parser.cjs.js",
      "types": "./dist/parser.d.ts"
    }
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": ">=11.5.2"
  },
  "volta": {
    "node": "22.18.0",
    "npm": "11.5.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./src/timetable/proto/timetable.proto; protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. ./src/stops/proto/stops.proto",
    "build": "npm run clean && npm run build:proto && npm run type:dts && npm run build:src",
    "build:src": "rollup -c",
    "clean": "rimraf build coverage",
    "type:dts": "tsc --emitDeclarationOnly --project tsconfig.build.json",
    "type:check": "tsc --noEmit",
    "lint": "eslint src --fix",
    "lint:check": "eslint src",
    "format:check": "prettier . --check",
    "format": "prettier --write .",
    "test": "tsx --test src/**/__tests__/*.test.ts",
    "e2e": "tsx --test src/__e2e__/*.test.ts",
    "perf": "tsx src/cli/minotor perf -s src/__e2e__/timetable/stops.bin -t src/__e2e__/timetable/timetable.bin src/__e2e__/benchmark.json",
    "repl": "tsx src/cli/minotor repl -s src/__e2e__/timetable/stops.bin -t src/__e2e__/timetable/timetable.bin",
    "test:watch": "tsx --watch --test src/**/*.test.ts",
    "test:coverage": "c8 -x 'src/**/proto/*.ts' -x 'src/**/__tests__/*.ts' --check-coverage --lines=85 tsx --test src/**/__tests__/*.test.ts",
    "spell:check": "cspell \"{README.md,.github/*.md,src/**/*.ts}\" -u",
    "cz": "cz",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@ryansonshine/commitizen": "^4.2.8",
    "@ryansonshine/cz-conventional-changelog": "^3.3.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/github": "^11.0.4",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/geokdbush": "^1.1.5",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.3.0",
    "c8": "^10.1.3",
    "cspell": "^9.2.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.49.0",
    "semantic-release": "^24.2.7",
    "ts-proto": "^2.7.7",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.7.0",
    "commander": "^14.0.0",
    "csv-parse": "^6.1.0",
    "geokdbush": "^2.0.1",
    "kdbush": "^4.0.2",
    "loglevel": "^1.9.2",
    "luxon": "^3.7.1",
    "node-stream-zip": "^1.15.0",
    "slimsearch": "^2.2.2"
  }
}
